{
	"compilerOptions": {
		"target": "es2016",
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"baseUrl": "./src",
		"paths": {
			"@/*": ["./*"],
			"@routes/*": ["routes/*"],
			"@modules/*": ["modules/*"],
			"@utils/*": ["utils/*"]
		}
	}
}
